<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-ahshipin" version="1.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>dhshipin</name>
    <js-module name="dhshipin" src="www/dhshipin.js">
        <clobbers target="cordova.plugins.dhshipin" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
        <feature name="dhshipin">
            <param name="ios-package" value="dhshipin" />
        </feature>
        </config-file>
        <source-file src="src/ios/dhshipin.m" />
    </platform>
</plugin>
